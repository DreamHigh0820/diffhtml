{
  "name": "diffhtml-components",
  "main": "dist/cjs/index.js",
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "derequire": "^2.0.6",
    "watchify": "^3.8.0"
  },
  "scripts": {
    "cjs": "babel lib -d dist/cjs",
    "build": "browserify -t babelify -s components lib/index.js | derequire > dist/components.js",
    "watch": "watchify -t babelify -s components lib/index.js -o 'derequire > dist/components.js' -v"
  },
  "dependencies": {
    "proptypes": "^0.14.3"
  }
}
