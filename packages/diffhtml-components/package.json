{
  "name": "diffhtml-components",
  "main": "dist/diffhtml-components.js",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "derequire": "^2.0.6",
    "watchify": "^3.8.0"
  },
  "scripts": {
    "build": "browserify -t babelify -s diffComponents index.js | derequire > dist/diffhtml-components.js",
    "watch": "watchify -t babelify -s diffComponents index.js -o 'derequire > dist/diffhtml-components.js' -v"
  }
}
