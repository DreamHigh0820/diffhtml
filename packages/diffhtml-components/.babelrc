{
  "env": {
    "cjs": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "transform-object-rest-spread",
        "transform-class-properties",
        ["module-rewrite", { "replaceFunc": "../../utils/replace-cjs.js" }],
      ]
    },

    "es": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        ["module-rewrite", { "replaceFunc": "../../utils/replace-es.js" }],
        "transform-object-rest-spread",
        "transform-class-properties"
      ]
    },

    "test": {
      "plugins": [
        ["module-rewrite", { "replaceFunc": "../../utils/replace-cjs.js" }],
        "transform-es2015-modules-commonjs",
        "transform-object-rest-spread",
        "transform-class-properties",
        ["transform-react-jsx", { "pragma": "createTree" }]
      ]
    }
  }
}
