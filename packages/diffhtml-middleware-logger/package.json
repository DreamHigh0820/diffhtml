{
  "name": "diffhtml-middleware-logger",
  "version": "1.0.0-beta",
  "description": "",
  "main": "dist/cjs/index",
  "module": "dist/es/index",
  "jsnext:main": "dist/es/index",
  "esnext:main": "dist/es/index",
  "scripts": {
    "build-cjs": "NODE_ENV=cjs babel index.js -d dist/cjs",
    "build-es": "NODE_ENV=es babel index.js -d dist/es",
    "build": "NODE_ENV=build browserify -t babelify -s logger index.js -o dist/logger.js && npm run build-cjs && npm run build-es",
    "watch": "watchify -t babelify -s logger index.js -o dist/logger.js -v"
  },
  "keywords": [
    "diffhtml",
    "logger",
    "middleware"
  ],
  "author": "Tim Branyen (@tbranyen)",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "coveralls": "^2.11.9",
    "derequire": "^2.0.3",
    "diffhtml": "^1.0.0-beta",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3"
  },
  "peerDependencies": {
    "diffhtml": "^1.0.0-beta"
  }
}
