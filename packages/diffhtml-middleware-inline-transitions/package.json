{
  "name": "diffhtml-middleware-inline-transitions",
  "version": "1.0.0-beta",
  "description": "Monitors inline attributes and assigns transition hooks",
  "main": "dist/inline-transitions.js",
  "scripts": {
    "build-cjs": "NODE_ENV=cjs babel index.js -d dist/cjs",
    "build-es": "NODE_ENV=es babel index.js -d dist/es",
    "build": "NODE_ENV=build browserify -t babelify -s inlineTransitions index.js -o dist/inline-transitions.js && npm run build-cjs && npm run build-es",
    "watch": "watchify -t babelify -s inlineTransitions index.js -o dist/inline-transitions.js -v",
    "mocha": "mocha test/_setup test/*.js",
    "test": "NODE_ENV=cjs istanbul cover _mocha -- -- test/_setup test/*.js",
    "test:ci": "npm run test"
  },
  "keywords": [
    "diffhtml",
    "transitions"
  ],
  "author": "Tim Branyen (@tbranyen)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "coveralls": "^2.11.15",
    "derequire": "^2.0.6",
    "diffhtml": "^1.0.0-beta",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom-wc": "^11.0.0-alpha-1",
    "mocha": "^3.2.0",
    "watchify": "^3.9.0"
  },
  "peerDependencies": {
    "diffhtml": ">=1.0.0-beta"
  }
}
