{
  "name": "diffhtml",
  "version": "0.4.2",
  "description": "",
  "main": "dist/diffhtml",
  "author": "Tim Branyen (@tbranyen)",
  "repository": "https://github.com/tbranyen/diffhtml",
  "license": "MIT",
  "scripts": {
    "start": "http-server",
    "build": "browserify -t [ babelify --presets [ es2015 ] ] -s diff lib/index.js | derequire > dist/diffhtml.js",
    "watch": "watchify -t [ babelify --presets [ es2015 ] ] -s diff lib/index.js -o dist/diffhtml.js -v",
    "jshint": "jshint lib/**/*.js",
    "test": "npm run jshint && karma start --single-run"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "browserify-istanbul": "^0.2.1",
    "coveralls": "^2.11.6",
    "custom-event": "^1.0.0",
    "http-server": "^0.8.5",
    "isparta": "^4.0.0",
    "jshint": "^2.8.0",
    "karma": "^0.13.19",
    "karma-browserify": "^4.4.2",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.3",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "^0.0.1",
    "promise-polyfill": "^2.1.4",
    "watchify": "^3.6.1",
    "weakmap": "^0.0.6"
  }
}
