{
  "name": "diffhtml",
  "version": "0.8.4",
  "description": "Easily swap out markup with an intelligent virtual dom",
  "main": "dist/diffhtml",
  "author": "Tim Branyen (@tbranyen)",
  "repository": "https://github.com/tbranyen/diffhtml",
  "license": "MIT",
  "scripts": {
    "start": "http-server",
    "build": "browserify -t babelify -s diff lib/index.js | derequire > dist/diffhtml.js",
    "prepublish": "babel lib --source-maps -d dist/cjs",
    "watch": "watchify -t babelify -s diff lib/index.js -o 'derequire > dist/diffhtml.js' -v",
    "jshint": "jshint lib/**/*.js",
    "karma": "karma start",
    "test": "npm run jshint && npm run karma -- --single-run"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-istanbul": "^2.0.0",
    "coveralls": "^2.11.9",
    "derequire": "^2.0.3",
    "es6-collections": "^0.5.6",
    "http-server": "^0.9.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.3",
    "jshint": "^2.9.2",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.5",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "promise-polyfill": "^5.2.1",
    "watchify": "^3.7.0"
  }
}
